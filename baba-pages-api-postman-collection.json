{
  "info": {
    "name": "Baba Pages API",
    "description": "Complete API collection for managing spiritual leader (Baba) pages with posts, videos, and stories",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Baba Management",
      "item": [
        {
          "name": "Create Baba Page",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"babaId\": \"baba_ramdev\",\n  \"babaName\": \"Baba Ramdev\",\n  \"spiritualName\": \"Swami Ramdev\",\n  \"description\": \"Yoga guru and spiritual leader\",\n  \"location\": \"Haridwar, India\",\n  \"ashram\": \"Patanjali Yogpeeth\",\n  \"socialLinks\": {\n    \"website\": \"https://patanjaliyogpeeth.net\",\n    \"youtube\": \"https://youtube.com/babarramdev\"\n  },\n  \"contactInfo\": {\n    \"email\": \"contact@patanjaliyogpeeth.net\",\n    \"phone\": \"+91-1234567890\"\n  },\n  \"spiritualTeachings\": [\"Yoga\", \"Meditation\", \"Ayurveda\"],\n  \"languages\": [\"Hindi\", \"English\", \"Sanskrit\"],\n  \"createdBy\": \"{{userId}}\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/baba",
              "host": ["{{base_url}}"],
              "path": ["api", "baba"]
            }
          }
        },
        {
          "name": "Get All Baba Pages",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/api/baba?page=1&limit=10",
              "host": ["{{base_url}}"],
              "path": ["api", "baba"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "10"
                }
              ]
            }
          }
        },
        {
          "name": "Get Specific Baba",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/api/baba?babaId={{babaId}}",
              "host": ["{{base_url}}"],
              "path": ["api", "baba"],
              "query": [
                {
                  "key": "babaId",
                  "value": "{{babaId}}"
                }
              ]
            }
          }
        },
        {
          "name": "Search Baba Pages",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/api/baba?search=yoga&page=1&limit=10",
              "host": ["{{base_url}}"],
              "path": ["api", "baba"],
              "query": [
                {
                  "key": "search",
                  "value": "yoga"
                },
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "10"
                }
              ]
            }
          }
        },
        {
          "name": "Update Baba Page",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"babaId\": \"{{babaId}}\",\n  \"description\": \"Updated description\",\n  \"location\": \"Updated location\",\n  \"socialLinks\": {\n    \"website\": \"https://updated-website.com\",\n    \"youtube\": \"https://youtube.com/updated-channel\"\n  }\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/baba",
              "host": ["{{base_url}}"],
              "path": ["api", "baba"]
            }
          }
        },
        {
          "name": "Delete Baba Page",
          "request": {
            "method": "DELETE",
            "url": {
              "raw": "{{base_url}}/api/baba?babaId={{babaId}}",
              "host": ["{{base_url}}"],
              "path": ["api", "baba"],
              "query": [
                {
                  "key": "babaId",
                  "value": "{{babaId}}"
                }
              ]
            }
          }
        }
      ]
    },
